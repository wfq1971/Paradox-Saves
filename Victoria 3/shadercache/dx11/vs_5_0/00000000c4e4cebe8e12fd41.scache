float4 PixelToScreenSpace( float2 PixelPos )
{
	float2 Pos = PixelPos * 2.0 - ScreenDimension;
	Pos /= ScreenDimension;
	Pos.y = -Pos.y;
  
	return float4( Pos, 0.0, 1.0 );
